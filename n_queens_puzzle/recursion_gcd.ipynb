    "def recursion_gcd(m, n):\n",
    "    print(\"{}, {}\".format(m, n))\n",
    "    if m % n == 0:\n",
    "        return 0\n",
    "    else:\n",
    "        gcd = recursion_gcd(n, m  % n)\n",
    "        return gcd"
   ]
