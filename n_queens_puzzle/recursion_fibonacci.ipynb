    "def recursion_fibonacci(x):\n",
    "    if x == 0:\n",
    "        return 0\n",
    "    elif x == 1:\n",
    "        return 1\n",
    "    else:\n",
    "        y = recursion_fibonacci(x -1) + recursion_fibonacci(x -2)\n",
    "    return y"
   ]
